{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/no-unresolved": "warn",
    "import/prefer-default-export": "off",
    "no-shadow": "off",
    "react/forbid-prop-types": "off"
  },
  "globals": {
    "process": true,
    "__dirname": true,
    "document": true,
    "window": true
  }
}